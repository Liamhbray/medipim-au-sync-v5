# Supabase Configuration
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_ROLE_KEY=your-supabase-service-role-key

# Fetcher Service Configuration
MEDIPIM_API_URL=https://api.au.medipim.com/v4/products/stream
MEDIPIM_API_KEY_ID=your-medipim-api-key-id
MEDIPIM_API_KEY_SECRET=your-medipim-api-key-secret
ADMIN_KEY=your-secure-admin-key-here
FETCHER_PORT=3001
MAINTAINER_PORT=3002
ORCHESTRATOR_PORT=3003

# Processing Configuration (optional)
BATCH_SIZE=100
REQUEST_TIMEOUT=900000

# API Keys (Required to enable respective provider)
ANTHROPIC_API_KEY="your_anthropic_api_key_here"       # Required: Format: sk-ant-api03-...
PERPLEXITY_API_KEY="your_perplexity_api_key_here"     # Optional: Format: pplx-...
OPENAI_API_KEY="your_openai_api_key_here"             # Optional, for OpenAI/OpenRouter models. Format: sk-proj-...
GOOGLE_API_KEY="your_google_api_key_here"             # Optional, for Google Gemini models.
MISTRAL_API_KEY="your_mistral_key_here"               # Optional, for Mistral AI models.
XAI_API_KEY="YOUR_XAI_KEY_HERE"                       # Optional, for xAI AI models.
AZURE_OPENAI_API_KEY="your_azure_key_here"            # Optional, for Azure OpenAI models (requires endpoint in .taskmaster/config.json).
OLLAMA_API_KEY="your_ollama_api_key_here"             # Optional: For remote Ollama servers that require authentication.

# Email Notification Configuration (optional)
SMTP_SERVER=smtp.gmail.com                             # SMTP server address (e.g., smtp.gmail.com)
SMTP_PORT=587                                          # SMTP server port (e.g., 587 for TLS)
SMTP_USERNAME=your-email@gmail.com                     # SMTP username/email
SMTP_PASSWORD=your-app-password-here                   # SMTP password or app password
NOTIFICATION_EMAIL=notifications@example.com           # Email address to receive sync notifications